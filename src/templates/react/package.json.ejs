{
  "name": "<%= projectName %>",
  "version": "0.1.0",
  "private": true,
  "description": "<%= description || 'A React application built with ' + buildTool %>",
  "type": "module",
  "scripts": {
<% if (buildTool === 'vite') { %>
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
<% } else if (buildTool === 'webpack') { %>
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
<% } else if (buildTool === 'rollup') { %>
    "dev": "rollup -c -w",
    "build": "rollup -c",
<% } %>
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
<% if (features.includes('prettier')) { %>
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,scss,json}\"",
<% } %>
<% if (features.includes('testing')) { %>
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
<% } %>
<% if (features.includes('storybook')) { %>
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
<% } %>
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
<% if (features.includes('router')) { %>,
    "react-router-dom": "^6.8.0"
<% } %>
<% if (features.includes('state-management')) { %>,
    "zustand": "^4.4.0"
<% } %>
<% if (features.includes('ui-library')) { %>,
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0"
<% } %>
<% if (features.includes('icons')) { %>,
    "lucide-react": "^0.294.0"
<% } %>
<% if (features.includes('forms')) { %>,
    "react-hook-form": "^7.48.0",
    "@hookform/resolvers": "^3.3.0",
    "zod": "^3.22.0"
<% } %>
<% if (features.includes('http-client')) { %>,
    "axios": "^1.6.0"
<% } %>
<% if (features.includes('animations')) { %>,
    "framer-motion": "^10.16.0"
<% } %>
  },
  "devDependencies": {
<% if (buildTool === 'vite') { %>
    "@vitejs/plugin-react": "^4.2.0",
    "vite": "^5.0.0",
<% } else if (buildTool === 'webpack') { %>
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0",
    "html-webpack-plugin": "^5.5.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.23.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "style-loader": "^3.3.0",
<% } else if (buildTool === 'rollup') { %>
    "rollup": "^4.6.0",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@rollup/plugin-replace": "^5.0.0",
    "rollup-plugin-serve": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.0",
<% } %>
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.0"
<% if (features.includes('tailwind')) { %>,
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
<% } %>
<% if (features.includes('prettier')) { %>,
    "prettier": "^3.1.0",
    "eslint-config-prettier": "^9.1.0"
<% } %>
<% if (features.includes('testing')) { %>,
    "vitest": "^1.0.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/user-event": "^14.5.0",
    "jsdom": "^23.0.0"
<% } %>
<% if (features.includes('storybook')) { %>,
    "@storybook/react": "^7.6.0",
    "@storybook/react-vite": "^7.6.0",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-interactions": "^7.6.0",
    "@storybook/addon-links": "^7.6.0",
    "@storybook/blocks": "^7.6.0",
    "@storybook/testing-library": "^0.2.0"
<% } %>
  },
  "keywords": [
    "react",
    "typescript",
    "<%= buildTool %>"
<% features.forEach(feature => { %>,
    "<%= feature %>"
<% }) %>
  ],
  "author": "<%= author || '' %>",
  "license": "<%= license || 'MIT' %>"
<% if (repository) { %>,
  "repository": {
    "type": "git",
    "url": "<%= repository %>"
  }
<% } %>
}