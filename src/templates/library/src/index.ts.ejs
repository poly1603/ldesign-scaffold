// 格式化工具
export * from './format'

// 类型工具
export * from './types'

// 常量
export * from './constants'

// 库版本
export const version = '0.1.0'

// 库信息
export const libraryInfo = {
  name: '<%= projectName %>',
  version,
  description: '<%= description %>',
  author: '<%= author %>'
}

// 默认导出所有工具函数
export default {
  version,
  libraryInfo
}

<% } else { %>
// 通用库入口文件

// 导出主要功能
export * from './core'

// 导出工具函数
