{
  "name": "vue3-basic",
  "displayName": "Vue 3 基础模板",
  "description": "基于 Vue 3 + Vite + TypeScript 的现代化前端项目模板",
  "version": "1.0.0",
  "author": "LDesign Team",
  "type": "vue3",
  "features": [
    "vue3",
    "vite",
    "typescript",
    "vue-router",
    "pinia",
    "eslint",
    "prettier"
  ],
  "dependencies": {
    "vue": "^3.3.0",
    "vue-router": "^4.2.0",
    "pinia": "^2.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/tsconfig": "^0.4.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vue-tsc": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "vue3",
    "vite",
    "typescript"
  ]
}